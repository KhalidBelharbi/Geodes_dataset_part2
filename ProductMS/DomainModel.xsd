<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
   <xs:complexType name="product" >
      <xs:sequence>
        <xs:element name="prodid" type="xs:anyType" />
        <xs:element name="productname" type="xs:string" />
        <xs:element name="price" type="xs:double" />
        <xs:element name="stock" type="xs:anyType" />
        <xs:element name="description" type="xs:string" />
        <xs:element name="image" type="xs:string" />
        <xs:element name="sellerid" type="xs:string" />
        <xs:element name="category" type="xs:string" />
        <xs:element name="subcategory" type="xs:string" />
        <xs:element name="productrating" type="xs:anyType" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="subscribedproduct" >
      <xs:sequence>
        <xs:element name="buyerid" type="xs:string" />
        <xs:element name="prodid" type="xs:anyType" />
        <xs:element name="quantity" type="xs:anyType" />
      </xs:sequence>
   </xs:complexType>
</xs:schema> 