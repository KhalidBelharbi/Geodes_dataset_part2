      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="tag" >
      <xs:sequence>
        <xs:element name="tagid" type="xs:long" />
        <xs:element name="name" type="xs:string" />
        <xs:element name="articleid" type="xs:long" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="like" >
      <xs:sequence>
        <xs:element name="no" type="xs:long" />
