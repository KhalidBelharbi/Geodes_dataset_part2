        <xs:element name="tickets" type="exchangeticket" nillable="true" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="exchangeticket" >
      <xs:sequence>
        <xs:element name="idticket" type="xs:long" />
        <xs:element name="client" type="person" nillable="true" minOccurs="0" />
        <xs:element name="fromcurr" type="xs:string" />
        <xs:element name="tocurr" type="xs:string" />
        <xs:element name="tocurramount" type="xs:double" />
        <xs:element name="status" type="xs:string" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="hsqldbexception" >
