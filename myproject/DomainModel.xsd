      <xs:sequence>
        <xs:element name="bookingid" type="xs:long" />
        <xs:element name="userid" type="xs:long" />
        <xs:element name="serviceid" type="xs:long" />
        <xs:element name="productname" type="xs:string" />
        <xs:element name="servicetype" type="xs:string" />
