        <xs:element name="//uni-directionalmany-to-oneassociationto" type="category" nillable="true" minOccurs="0" />
        <xs:element name="//uni-directionalmany-to-oneassociationto" type="location" nillable="true" minOccurs="0" />
        <xs:element name="//uni-directionalmany-to-oneassociationto" type="motto" nillable="true" minOccurs="0" />
        <xs:element name="//uni-directionalmany-to-oneassociationto" type="timeinfo" nillable="true" minOccurs="0" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="location" >
