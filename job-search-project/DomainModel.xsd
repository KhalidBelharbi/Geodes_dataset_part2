      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="company" >
      <xs:sequence>
        <xs:element name="id" type="xs:long" />
        <xs:element name="name" type="xs:string" />
        <xs:element name="codeedrpou" type="xs:long" />
        <xs:element name="address" type="xs:string" />
        <xs:element name="description" type="xs:string" />
        <xs:element name="industry" type="xs:string" />
        <xs:element name="employeesnum" type="xs:anyType" />
        <xs:element name="//@onetomany(mappedby" type="job" nillable="true" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="job" >
      <xs:sequence>
        <xs:element name="id" type="xs:long" />
