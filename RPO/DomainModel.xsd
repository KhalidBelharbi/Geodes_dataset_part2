        <xs:element name="currentworkinghere" type="xs:anyType" />
        <xs:element name="workexperience" type="xs:string" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="interviewnotes" >
      <xs:sequence>
        <xs:element name="id" type="xs:long" />
        <xs:element name="notetype" type="xs:string" />
        <xs:element name="notedata" type="xs:string" />
        <xs:element name="registrationid" type="xs:long" />
