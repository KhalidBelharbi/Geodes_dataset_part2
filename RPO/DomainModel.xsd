<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
   <xs:complexType name="educationaldetails" >
      <xs:sequence>
        <xs:element name="/***/serialversionuid" type="xs:long" />
        <xs:element name="id" type="xs:long" />
        <xs:element name="nameofschool" type="xs:string" />
        <xs:element name="degree" type="addqualification" nillable="true" minOccurs="0" />
        <xs:element name="durationform" type="xs:dateTime" />
        <xs:element name="durationto" type="xs:dateTime" />
        <xs:element name="currentpursuing" type="xs:anyType" />
