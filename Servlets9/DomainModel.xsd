        <xs:element name="statusentries" type="statusentry" nillable="true" minOccurs="0" maxOccurs="unbounded" />
        <xs:element name="sessiondate" type="xs:anyType" />
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="persondetails" >
      <xs:sequence>
        <xs:element name="serialversionuid" type="xs:long" />
        <xs:element name="id" type="xs:long" />
        <xs:element name="GeneratedName" type="person" nillable="true" minOccurs="0" />
        <xs:element name="photo" type="xs:anyType" />
