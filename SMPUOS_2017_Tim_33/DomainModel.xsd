        <xs:element name="tip" type="rezervacijatip" nillable="true" minOccurs="0" />
        <xs:element name="brojsedista" type="xs:long" />
        <xs:element name="brojredasedista" type="xs:long" />
        <xs:element name="//@jsonbackreference" type="projekcija" nillable="true" minOccurs="0" />
      </xs:sequence>
   </xs:complexType>
</xs:schema> 