         </xs:sequence>
       </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="ptuprognosis" >
      <xs:complexContent>
       <xs:extension base="exchange">
         <xs:sequence>
          <xs:element name="type" type="prognosistype" nillable="true" minOccurs="0" />
          <xs:element name="power" type="xs:anyType" />
          <xs:element name="substitute" type="xs:anyType" />
         </xs:sequence>
       </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="connectiongroup" >
