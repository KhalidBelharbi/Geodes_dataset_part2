          <xs:element name="prognosissequence" type="xs:long" />
         </xs:sequence>
       </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="ptusettlement" >
      <xs:complexContent>
       <xs:extension base="document">
         <xs:sequence>
          <xs:element name="" type="flexordersettlement" nillable="true" minOccurs="0" />
