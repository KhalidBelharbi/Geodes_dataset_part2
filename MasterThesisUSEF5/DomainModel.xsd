          <xs:element name="actualpower" type="xs:anyType" />
          <xs:element name="orderedflexpower" type="xs:anyType" />
          <xs:element name="deliveredflexpower" type="xs:anyType" />
          <xs:element name="powerdeficiency" type="xs:anyType" />
