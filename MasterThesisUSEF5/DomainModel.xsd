        <xs:element name="ptu" type="xs:dateTime" />
        <xs:element name="ptuindex" type="xs:anyType" />
        <xs:element name="phase" type="phasetype" nillable="true" minOccurs="0" />
      </xs:sequence>
   </xs:complexType>
</xs:schema> 